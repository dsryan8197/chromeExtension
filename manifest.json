{
"manifest_version" : 2,
"name" : "New Title",
"version": "1.0",
"permissions": [
    "activeTab"
],
"description" : "Change a Title",
"content_scripts" : [
    {
        "matches" : ["<all_urls>"],
        "js" : ["chrome.js", "chrome.css", "chrome.html"]
    }
],

"browser_action" : {
    "default_popup": "chrome.html"
}

}